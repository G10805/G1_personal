cc_binary {
    name: "android.hardware.gnss-service.airoha",
    relative_install_path: "hw",
    init_rc: [
        "android.hardware.gnss-service.airoha.rc",
    ],
    proprietary: true,
    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror",
    ],
    cppflags: [
        "-fexceptions",
    ],
    shared_libs: [
        "libbase",
        "libcutils",
        "libbinder_ndk",
        "libhidlbase",
        "libutils",
        "liblog",
        "android.hardware.gnss@2.1",
        "android.hardware.gnss@2.0",
        "android.hardware.gnss@1.0",
        "android.hardware.gnss.measurement_corrections@1.1",
        "android.hardware.gnss.measurement_corrections@1.0",
        "android.hardware.gnss.visibility_control@1.0",
    ],
    srcs: [
        "src/AGnssRil.cpp",
        "src/AGnss.cpp",
        "src/Gnss.cpp",
        "src/GnssAntennaInfo.cpp",

        "src/GnssDebug.cpp",
        "src/GnssGeofence.cpp",
        "src/GnssNavigationMessageInterface.cpp",
        "src/GnssPowerIndication.cpp",
        "src/GnssPsds.cpp",
        "src/GnssConfiguration.cpp",
        "src/GnssMeasurementInterface.cpp",
        "src/GnssVisibilityControl.cpp",
        "src/MeasurementCorrectionsInterface.cpp",
        "src/service.cpp",
        "src/GnssAidlImpl.cpp",
        "src/BuildVersion.cpp",
        "src/GnssSimulate.cpp",
    ],
    local_include_dirs: ["inc"],
    static_libs: [
        "libairohacommon",
        "libairoha_prop",
        "libc++_static-airoha",
    ],
    defaults: [
        "aidl_gnss_default"
    ],
}
airoha_aidl_default {
    name: "aidl_gnss_default",
    shared_libs: []
}
bootstrap_go_package {
    name: "snoog-airoha_aidl_default",
    pkgPath: "android/soong/airoha/property",
    deps: [
        "soong-android",
        "soong-cc",
    ],
    srcs :[
        "aidl_property.go",
    ],
    pluginFor: ["soong_build"]
}
