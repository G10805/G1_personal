SectionVerb {
    EnableSequence [
    ]
    DisableSequence [
    ]
    value {
    }
}

SectionDevice."Speaker" {
    EnableSequence [

    ]
    DisableSequence [

    ]
    value {
    }
}

SectionModifier."HiFi" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM0p control' ZERO"
        cset-tlv "name='PCM0p metadata' /etc/acdb-blob/hifi_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-RX-PRIMARY metadata' /etc/acdb-blob/hifi_Meta_device.bin"
        cset "name='PCM0p control' TDM-LPAIF-RX-PRIMARY"
        cset-tlv "name='PCM0p metadata' /etc/acdb-blob/hifi_Meta_streamdevice.bin"
        cset "name='PCM0p connect' TDM-LPAIF-RX-PRIMARY"
    ]
    DisableSequence [
    ]
    value {
        playbackPCM "agm:100,0"
    }
}

SectionModifier."Record" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM0c control' ZERO"
        cset-tlv "name='PCM0c metadata' /etc/acdb-blob/record_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-TX-PRIMARY metadata' /etc/acdb-blob/record_Meta_device.bin"
        cset "name='PCM0c connect' TDM-LPAIF-TX-PRIMARY"
    ]
    DisableSequence [

    ]
    value {
        CapturePCM "agm:100,100"
        CaptureRate "48000"
    }
}

SectionModifier."Navigation" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM1p control' ZERO"
        cset-tlv "name='PCM1p metadata' /etc/acdb-blob/navi_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-RX-PRIMARY metadata' /etc/acdb-blob/navi_Meta_device.bin"
        cset "name='PCM1p control' TDM-LPAIF-RX-PRIMARY"
        cset-tlv "name='PCM1p metadata' /etc/acdb-blob/navi_Meta_streamdevice.bin"
        cset "name='PCM1p connect' TDM-LPAIF-RX-PRIMARY"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,1"
    }
}

SectionModifier."ICC" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/icc_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-RX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_rx.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF-RX-PRIMARY"
        cset "name='TDM-LPAIF-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF-TX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_tx.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
    }
}