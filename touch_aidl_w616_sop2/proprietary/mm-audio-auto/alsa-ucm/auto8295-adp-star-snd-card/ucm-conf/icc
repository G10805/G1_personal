SectionVerb {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/icc_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-RX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_rx.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF-RX-PRIMARY"
        cset "name='TDM-LPAIF-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF-TX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_tx.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
        ]
    DisableSequence [
    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
    }
}

