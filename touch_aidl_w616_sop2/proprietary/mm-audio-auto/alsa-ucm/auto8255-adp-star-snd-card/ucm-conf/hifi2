SectionVerb {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-QUATERNARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM4p control' ZERO"
        cset-tlv "name='PCM4p metadata' /etc/acdb-blob/hifi2_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-QUATERNARY metadata' /etc/acdb-blob/hifi2_Meta_device.bin"
        cset "name='PCM4p connect' TDM-LPAIF_SDR-RX-QUATERNARY"
    ]
    DisableSequence [
    ]
    value {
        playbackPCM "agm:100,4"
    }
}

SectionDevice."Speaker" {
    EnableSequence [

    ]
    DisableSequence [

    ]
    value {
        PlaybackChannels "6"
    }
}