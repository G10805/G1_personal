    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-TERTIARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM0p control' ZERO"
        cset-tlv "name='PCM0p metadata' /etc/acdb-blob/hifi_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-TERTIARY metadata' /etc/acdb-blob/hifi_Meta_device.bin"
        cset "name='PCM0p control' TDM-LPAIF_SDR-RX-TERTIARY"
        cset-tlv "name='PCM0p metadata' /etc/acdb-blob/hifi_Meta_streamdevice.bin"
        cset "name='PCM0p connect' TDM-LPAIF_SDR-RX-TERTIARY"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,0"
    }
}

