SectionModifier."ICC" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-TERTIARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/icc_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-TERTIARY metadata' /etc/acdb-blob/icc_Meta_device_rx.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF_SDR-RX-TERTIARY"
        cset "name='TDM-LPAIF-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF-TX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_tx.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
    }
}

SectionModifier."Hfp_wb" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-TERTIARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/hfp_wb_Meta_stream_downlink_rx.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-TERTIARY metadata' /etc/acdb-blob/hfp_wb_Meta_device_downlink_rx.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF_SDR-RX-TERTIARY"
        cset "name='TDM-LPAIF_WSA-TX-PRIMARY rate ch fmt' 16000,8,2,1"
        cset-tlv "name='TDM-LPAIF_WSA-TX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_Meta_device_downlink_tx.bin"
        cset-tlv "name='HOSTLESS0c metadata' /etc/acdb-blob/hfp_wb_Meta_stream_downlink_tx.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF_WSA-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
        cset "name='TDM-LPAIF_WSA-RX-PRIMARY rate ch fmt' 16000,8,2,1"
        cset "name='HOSTLESS1p control' ZERO"
        cset-tlv "name='TDM-LPAIF_WSA-RX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_Meta_device_uplink_rx.bin"
        cset "name='HOSTLESS1p connect' TDM-LPAIF_WSA-RX-PRIMARY"
        cset "name='TDM-LPAIF-TX-SECONDARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF-TX-SECONDARY metadata' /etc/acdb-blob/hfp_wb_Meta_device_uplink_tx.bin"
        cset "name='HOSTLESS1c connect' TDM-LPAIF-TX-SECONDARY"
        cset "name='HOSTLESS1c loopback' HOSTLESS1p"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
        playbackPCM "agm:100,201"
        CapturePCM "agm:100,301"
    }
}
