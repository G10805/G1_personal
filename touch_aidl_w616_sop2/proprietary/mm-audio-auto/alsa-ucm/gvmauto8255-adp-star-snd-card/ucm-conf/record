SectionVerb {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF-TX-SECONDARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM0c control' ZERO"
        cset-tlv "name='PCM0c metadata' /etc/acdb-blob/record_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF-TX-SECONDARY metadata' /etc/acdb-blob/record_Meta_device.bin"
        cset "name='PCM0c connect' TDM-LPAIF-TX-SECONDARY"
    ]
    DisableSequence [
    ]
    value {
        CapturePCM "agm:100,100"
        CaptureRate "48000"
    }
}

SectionDevice."Microphone" {
    EnableSequence [

    ]
    DisableSequence [

    ]
    value {
        CaptureChannels "1"
    }
}

