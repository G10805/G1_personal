SectionModifier."ICC" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/icc_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_rx.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF_SDR-RX-PRIMARY"
        cset "name='TDM-LPAIF_SDR-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF_SDR-TX-PRIMARY metadata' /etc/acdb-blob/icc_Meta_device_tx.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF_SDR-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
    ]
    DisableSequence [

    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
    }
}

SectionModifier."Hfp_wb" {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-RX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset "name='HOSTLESS0p control' ZERO"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/hfp_wb_downlink_rx_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-RX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_downlink_rx_Meta_device.bin"
        cset "name='HOSTLESS0p control' TDM-LPAIF_SDR-RX-PRIMARY"
        cset-tlv "name='HOSTLESS0p metadata' /etc/acdb-blob/hfp_wb_downlink_rx_Meta_streamdevice.bin"
        cset "name='HOSTLESS0p connect' TDM-LPAIF_SDR-RX-PRIMARY"
        cset "name='TDM-LPAIF_AUD-TX-PRIMARY rate ch fmt' 16000,8,2,1"
        cset-tlv "name='TDM-LPAIF_AUD-TX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_downlink_tx_Meta_device.bin"
        cset-tlv "name='HOSTLESS0c metadata' /etc/acdb-blob/hfp_wb_downlink_tx_Meta_stream.bin"
        cset "name='HOSTLESS0c connect' TDM-LPAIF_AUD-TX-PRIMARY"
        cset "name='HOSTLESS0c loopback' HOSTLESS0p"
        cset "name='TDM-LPAIF_AUD-RX-PRIMARY rate ch fmt' 16000,8,2,1"
        cset "name='HOSTLESS1p control' ZERO"
        cset-tlv "name='TDM-LPAIF_AUD-RX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_uplink_rx_Meta_device.bin"
        cset "name='HOSTLESS1p connect' TDM-LPAIF_AUD-RX-PRIMARY"
        cset "name='TDM-LPAIF_SDR-TX-PRIMARY rate ch fmt' 48000,8,2,1"
        cset-tlv "name='TDM-LPAIF_SDR-TX-PRIMARY metadata' /etc/acdb-blob/hfp_wb_uplink_tx_Meta_device.bin"
        cset "name='HOSTLESS1c control' TDM-LPAIF_SDR-TX-PRIMARY"
        cset-tlv "name='HOSTLESS1c metadata' /etc/acdb-blob/hfp_wb_uplink_tx_Meta_streamdevice.bin"
        cset "name='HOSTLESS1c connect' TDM-LPAIF_SDR-TX-PRIMARY"
        cset "name='HOSTLESS1c loopback' HOSTLESS1p"
        ]
    DisableSequence [
    ]
    value {
        playbackPCM "agm:100,200"
        CapturePCM "agm:100,300"
        playbackPCM "agm:100,201"
        CapturePCM "agm:100,301"
    }
}
