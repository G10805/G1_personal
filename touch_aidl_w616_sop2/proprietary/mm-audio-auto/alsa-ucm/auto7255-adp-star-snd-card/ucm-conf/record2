SectionVerb {
    EnableSequence [
        cdev "agm"
        cset "name='TDM-LPAIF_SDR-TX-QUATERNARY rate ch fmt' 48000,8,2,1"
        cset "name='PCM3c control' ZERO"
        cset-tlv "name='PCM3c metadata' /etc/acdb-blob/record2_Meta_stream.bin"
        cset-tlv "name='TDM-LPAIF_SDR-TX-QUATERNARY metadata' /etc/acdb-blob/record2_Meta_device.bin"
        cset "name='PCM3c connect' TDM-LPAIF_SDR-TX-QUATERNARY"
    ]
    DisableSequence [
    ]
    value {
        CapturePCM "agm:100,103"
        CaptureRate "48000"
    }
}

SectionDevice."Microphone" {
    EnableSequence [

    ]
    DisableSequence [

    ]
    value {
        CaptureChannels "1"
    }
}
