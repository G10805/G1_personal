SectionVerb {
	EnableSequence [
		cdev "hw:0"
		cset "name='TERT_TDM SlotWidth' 32"
		cset "name='TERT_TDM SlotNumber' Eight"
		cset "name='TERT_TDM_TX_0 SampleRate' KHZ_48"
		cset "name='TERT_TDM_TX_0 SlotMapping' 0,4,8,12,16,20,65535"
		cset "name='TERT_TDM_TX_0 Channels' Four"
		cset "name='MultiMedia1 Mixer TERT_TDM_TX_0' 1"
		cset "name='App Type Config' 1,69938,48000,16"
		cset "name='Audio Stream Capture 0 App Type Cfg' 69938,47,48000,88"
	]

	DisableSequence [
		cdev "hw:0"
		cset "name='MultiMedia1 Mixer TERT_TDM_TX_0' 0"
	]

	Value {
		CapturePCM "hw:0,0"
		CaptureRate "48000"
	}
}

