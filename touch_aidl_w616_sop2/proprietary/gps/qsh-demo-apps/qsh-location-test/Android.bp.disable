cc_binary {
    name: "qsh_location_test",
    clang: true,
    owner: "qti",
    proprietary: true,
    srcs: [
        "src/qsh_location_test.cpp",
    ],
    cflags: [
        "-Wall",
        "-Wextra",
        "-Werror",
        "-Wno-missing-field-initializers",
        "-Wno-maybe-uninitialized",
        "-Wunused-private-field",
    ] + GNSS_CFLAGS,
    cppflags: ["-fexceptions"],
    header_libs: [
        "libssc_headers",
        "libqmi_common_headers",
        "libgps.utils_headers",
        "libloc_pla_headers",
        "libqshutils_headers"
    ],
    shared_libs: [
        "libutils",
        "libcutils",
        "liblog",
        "libgps.utils",
        "libsnsapi",
        "libqmi_encdec",
        "libqmi_cci",
        "libqshlocation",
        "libqshutils",
        "libprotobuf-cpp-lite"
    ],
}
