on property:init.svc.mediadrm=running
    mkdir /data/vendor/mediadrm 0770 media mediadrm

service vendor.drm-playready-hal /vendor/bin/hw/android.hardware.drm-service.playready-qti
    interface aidl android.hardware.drm::IDrmFactory/playready
    class hal
    user media
    group media mediadrm drmrpc system
    ioprio rt 4
    task_profiles ProcessCapacityHigh
