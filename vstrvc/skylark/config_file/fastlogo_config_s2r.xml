		</module>
		-->
		<module name="drm">
			<config name="src_w" type="int" value="1920"/>
			<config name="src_h" type="int" value="720"/>
			<config name="dst_w" type="int" value="1920"/>
			<config name="dst_h" type="int" value="720"/>
			<config name="fourcc" type="DRM" value="BGR888"/>
			<config name="plane_number" type="int" value="3"/>
			<config name="null_pause" type="int" value="1"/>
			<config name="frame_rate" type="int" value="30"/>
		</module>
	</h264_logo>
	
	<raw_logo>
             <module name="fin">
                     <config name="name" type="string" value="/vendor/fastlogo_file/skylark_1540x720.BGR"/>
             </module>

             <module name="drm">
                     <config name="src_w" type="int" value="1920"/>
                     <config name="src_h" type="int" value="736"/>
                     <config name="dst_w" type="int" value="1920"/>
                     <config name="dst_h" type="int" value="736"/>
                     <config name="fourcc" type="DRM" value="RGB888"/>
                     <config name="plane_number" type="int" value="2"/>
                     <config name="null_pause" type="int" value="1"/>
             </module>
     </raw_logo>

	<setting>
		<log_level value="-1"/>
		<home_daemon value="surfaceflinger"/>
